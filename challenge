
#include<stdio.h>
#include<conio.h>
#include<ctype.h>
#include<stdlib.h>
#include<stdbool.h>


int main(){
  int  i, j, tmp;
  int array[]={23,67,89,5,45,20,15,27};
   int n=sizeof(array)/sizeof(array[0]);
printf("before:\n");
  for (i = 0; i <= n-1; i++){
    printf("%d ", array[i]);
  }
  for (i = 0; i <= n; i++){
    j = i;
    while(j > 0 && array[j-1] > array[j]){
      tmp = array[j];
      array[j] = array[j-1];
      array[j-1] = tmp;
      j--;
    }
  }
  printf("\nafter:\n");
  for (i = 0; i <= n-1; i++){
    printf("%d ", array[i]);
  }
  printf("\n");

  return 0;
}




